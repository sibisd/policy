{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"logs:Describe*",
				"logs:Get*",
				"logs:CreateLogGroup",
				"logs:DeleteLogGroup",
				"logs:PutRetentionPolicy",
				"logs:DeleteRetentionPolicy",
				"logs:TagLogGroup",
				"logs:UntagLogGroup",
				"logs:CreateLogStream",
				"logs:DeleteLogStream",
				"logs:PutLogEvents",
				"logs:FilterLogEvents",
				"logs:PutSubscriptionFilter",
				"logs:DeleteSubscriptionFilter",
				"logs:PutMetricFilter",
				"logs:DeleteMetricFilter",
				"logs:StartQuery",
				"logs:StopQuery"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"apigateway:DELETE",
				"apigateway:UpdateRestApiPolicy",
				"apigateway:PUT",
				"apigateway:PATCH",
				"apigateway:POST",
				"apigateway:GET"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"apigateway:DELETE",
				"apigateway:UpdateRestApiPolicy",
				"apigateway:PUT",
				"apigateway:PATCH",
				"apigateway:POST",
				"apigateway:GET"
			],
			"Resource": "arn:aws:apigateway:eu-west-2:*:/restapis/*/stages/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:PutEncryptionConfiguration",
				"s3:PutLifecycleConfiguration",
				"s3:PutBucketPublicAccessBlock",
				"s3:PutBucketOwnershipControls",
				"s3:get*",
				"s3:list*",
				"s3:CreateBucket",
				"s3:DeleteBucket",
				"s3:PutBucketPolicy",
				"s3:DeleteBucketPolicy",
				"s3:PutBucketTagging",
				"s3:PutBucketVersioning",
				"s3:PutObject",
				"s3:DeleteObject",
				"s3:AbortMultipartUpload"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"kms:EnableKeyRotation",
				"kms:ScheduleKeyDeletion",
				"kms:CreateAlias",
				"kms:CreateKey",
				"kms:TagResource",
				"kms:PutKeyPolicy",
				"kms:Describe*",
				"kms:List*",
				"kms:get*",
				"kms:EnableKey",
				"kms:DisableKey",
				"kms:CancelKeyDeletion",
				"kms:Encrypt",
				"kms:Decrypt"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:TagResource",
				"dynamodb:UpdateContinuousBackups",
				"dynamodb:CreateTable",
				"dynamodb:DeleteTable",
				"dynamodb:UpdateTable",
				"dynamodb:List*",
				"dynamodb:Describe*"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"codecommit:CreateRepository",
				"codecommit:GetRepository",
				"codecommit:TagResource"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"amplify:GetBranch",
				"amplify:ListJobs",
				"amplify:UntagResource",
				"amplify:DeleteBranch",
				"amplify:StartDeployment",
				"amplify:CreateDeployment",
				"amplify:CreateBranch",
				"amplify:UpdateBranch",
				"amplify:CreateWebHook",
				"amplify:ListTagsForResource",
				"amplify:TagResource"
			],
			"Resource": "arn:aws:amplify:eu-west-2:084828586425:apps/*/branches/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"amplify:DeleteDomainAssociation",
				"amplify:GetJob",
				"amplify:UntagResource",
				"amplify:DeleteWebHook",
				"amplify:UpdateWebHook",
				"amplify:UpdateDomainAssociation",
				"amplify:StartJob",
				"amplify:CreateDomainAssociation",
				"amplify:GetWebHook",
				"amplify:GetDomainAssociation",
				"amplify:DeleteJob",
				"amplify:ListTagsForResource",
				"amplify:TagResource",
				"amplify:StopJob"
			],
			"Resource": [
				"arn:aws:amplify:eu-west-2:084828586425:webhooks/*",
				"arn:aws:amplify:eu-west-2:084828586425:apps/*/branches/*/jobs/*",
				"arn:aws:amplify:eu-west-2:084828586425:apps/*/domains/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"amplify:GetArtifactUrl",
				"amplify:UntagResource",
				"amplify:DeleteBackendEnvironment",
				"amplify:ListDomainAssociations",
				"amplify:ListBackendEnvironments",
				"amplify:GetBackendEnvironment",
				"amplify:DeleteApp",
				"amplify:ListWebHooks",
				"amplify:ListBranches",
				"amplify:CreateBackendEnvironment",
				"amplify:GenerateAccessLogs",
				"amplify:ListArtifacts",
				"amplify:CreateApp",
				"amplify:ListTagsForResource",
				"amplify:TagResource",
				"amplify:GetApp",
				"amplify:UpdateApp"
			],
			"Resource": "arn:aws:amplify:eu-west-2:084828586425:apps/*"
		},
		{
			"Effect": "Allow",
			"Action": "amplify:ListApps",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ec2:CreateSecurityGroup",
				"ec2:DeleteSecurityGroup",
				"ec2:DescribeSecurityGroups",
				"ec2:AuthorizeSecurityGroupIngress",
				"ec2:AuthorizeSecurityGroupEgress",
				"ec2:RevokeSecurityGroupIngress",
				"ec2:RevokeSecurityGroupEgress",
				"ec2:ModifyInstanceAttribute",
				"ec2:ModifyNetworkInterfaceAttribute"
			],
			"Resource": [
				"arn:aws:ec2:eu-west-2:084828586425:security-group/*",
				"arn:aws:ec2:eu-west-2:084828586425:network-interface/*",
				"arn:aws:ec2:eu-west-2:084828586425:instance/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"ec2:CreateVpcEndpoint",
				"ec2:DescribeVpcEndpoints",
				"ec2:ModifyVpcEndpoint",
				"ec2:DeleteVpcEndpoints"
			],
			"Resource": "arn:aws:ec2:eu-west-2:084828586425:vpc-endpoint/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:CreateRepository",
				"ecr:DeleteRepository",
				"ecr:DescribeRepositories",
				"ecr:GetRepositoryPolicy",
				"ecr:ListImages",
				"ecr:DescribeImages",
				"ecr:DeleteRepositoryPolicy"
			],
			"Resource": "arn:aws:ecr:eu-west-2:084828586425:repository/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:BatchGetImage",
				"ecr:BatchCheckLayerAvailability",
				"ecr:PutImage",
				"ecr:InitiateLayerUpload",
				"ecr:UploadLayerPart",
				"ecr:CompleteLayerUpload"
			],
			"Resource": "arn:aws:ecr:eu-west-2:084828586425:repository/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:GetAuthorizationToken"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"ecr:GetDownloadUrlForLayer"
			],
			"Resource": "arn:aws:ecr:eu-west-2:084828586425:repository/*"
		},		
		{
			"Effect": "Allow",
			"Action": [
				"lambda:InvokeFunction",
				"lambda:List*",
				"lambda:Get*",
				"lambda:CreateFunction",
				"lambda:UpdateFunctionCode",
				"lambda:UpdateFunctionConfiguration",
				"lambda:DeleteFunction",
				"lambda:PublishVersion",
				"lambda:CreateAlias",
				"lambda:UpdateAlias",
				"lambda:PublishLayerVersion"
			],
			"Resource": "arn:aws:lambda:eu-west-2:084828586425:function:*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"sns:List*"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"sns:CreateTopic",
				"sns:DeleteTopic",
				"sns:Subscribe",
				"sns:Unsubscribe",
				"sns:SetTopicAttributes",
				"sns:GetTopicAttributes",
				"sns:TagResource",
				"sns:UntagResource",
				"sns:Publish"
			],
			"Resource": "arn:aws:sns:eu-west-2:084828586425:*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"sns:ConfirmSubscription",
				"sns:RemovePermission",
				"sns:AddPermission"
			],
			"Resource": "arn:aws:sns:eu-west-2:084828586425:*"
		}
		
	]
}